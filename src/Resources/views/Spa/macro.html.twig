{% macro jsResources() %}
    {% javascripts
        '@TaskStockBundle/Resources/public/js/components.jade.js'
        '@TaskStockBundle/Resources/assets/components/*/*.js'
        '@TaskStockBundle/Resources/assets/models/*/*.js'

        '@NotificationBundle/Resources/assets/models/notificationSchema/NotificationSchema.js'
        '@NotificationBundle/Resources/assets/models/notificationSchema/NotificationSchemaCollection.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% javascripts
        "../vendor/sokil/task-stock-bundle/src/Resources/public/js/messages.{locale}.js"
        vars=["locale"]
        output="js/taskStock.messages.{locale}.js"
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endmacro %}

{% macro cssResources() %}
    {% stylesheets
        '@TaskStockBundle/Resources/public/css/components.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endmacro %}
