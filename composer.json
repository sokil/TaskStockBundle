{
    "name": "sokil/task-tracker-bundle",
    "description": "Task tracker",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Sokil",
            "email": "dmytro.sokil@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "sokil/file-storage-bundle": "~0.1",
        "sokil/user-bundle": "~0.2",
        "sokil/notification-bundle": "~0.3",
        "sokil/frontend-bundle": "~0.12",
        "sokil/php-diff": "~0.3",
        "sokil/php-state": "~0.5",
        "sokil/php-upload": "~0.6",
        "sokil/php-symfony-validator-converter": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "satooshi/php-coveralls": "0.7.*"
    },
    "autoload": {
        "psr-4": {
            "Sokil\\TaskStockBundle\\": ["src/", "tests/"]
        }
    },
    "suggest": {
        "sokil/deploy-bundle": "Deploy symfony2 apps"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
